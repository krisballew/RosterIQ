# Copy to .env.local and fill in real values (never commit .env.local)

NEXT_PUBLIC_APP_URL=https://www.roster-iq.org

# Supabase — get from Supabase Dashboard > Project Settings > API
NEXT_PUBLIC_SUPABASE_URL=https://<your-project-ref>.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=<your-anon-key>
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>

# Dev-only harness secret — used to protect /api/dev/seed
ROSTERIQ_HARNESS_SECRET=dev-secret-change-me
